        <constraints nullable="false"/>
      </column>
      <column name="sink_type" type="VARCHAR(32)">
        <constraints nullable="false"/>
      </column>
      <column name="state" type="INT">
        <constraints nullable="false"/>
      </column>
      <column name="configuration" type="TEXT"/>
    </createTable>
  </changeSet>
</databaseChangeLog>
