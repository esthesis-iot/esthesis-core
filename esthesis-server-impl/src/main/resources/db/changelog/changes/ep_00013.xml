<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd"
  logicalFilePath="ep_00013.xml">
  <changeSet author="European Dynamics" id="1550559325733-4">
    <createTable tableName="virtualization">
      <column name="id" autoIncrement="true" type="BIGINT UNSIGNED">
        <constraints primaryKey="true"/>
      </column>
      <column name="name" type="VARCHAR(255)">
        <constraints nullable="false"/>
      </column>
      <column name="ip_address" type="VARCHAR(255)">
        <constraints nullable="false"/>
      </column>
      <column name="server_type" type="TINYINT(3)">
        <constraints nullable="false"/>
      </column>
      <column name="state" type="BIT(1)">
        <constraints nullable="false"/>
      </column>
      <column name="security" type="TINYINT(3)">
        <constraints nullable="false"/>
      </column>
      <column name="certificate" type="INT UNSIGNED"/>
    </createTable>
  </changeSet>
  <changeSet author="European Dynamics" id="1550559325733-5">
    <createTable tableName="virtualization_tags">
      <column name="virtualization_id" type="BIGINT UNSIGNED">
        <constraints primaryKey="true"/>
      </column>
      <column name="tag_id" type="BIGINT UNSIGNED">
        <constraints primaryKey="true"/>
      </column>
    </createTable>
  </changeSet>
</databaseChangeLog>
